{
  "name": "parse-md",
  "version": "1.0.0",
  "description": "Separate markdown file's metadata from its content",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rpearce/parse-md"
  },
  "scripts": {
    "test": "find ./test -name '*.test.js' | xargs mocha --require ./test/mocha-babel.js --recursive ./test",
    "clean": "rm -f index.js",
    "build": "babel lib/parse-md.js -o index.js",
    "prebuild": "npm run clean",
    "prepublish": "npm run build && npm test"
  },
  "keywords": [
    "markdown",
    "metadata",
    "md",
    "meta"
  ],
  "author": "Robert Pearce",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^3.4.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.4.0",
    "mocha": "^2.3.3"
  }
}
